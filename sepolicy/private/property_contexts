# Fastbootd
ro.fastbootd.available	u:object_r:exported_default_prop:s0

# RAM+
# NOTE: the sources map "persist.device_config.lmkd_native." properties to
# "device_config_lmkd_native_prop" property domain and almost everyone is
# "neverallow"ed to modify it, so we give this specific property a different
# context as a workaround
persist.device_config.lmkd_native.swap_free_low_percentage                  u:object_r:device_config_lmkd_native_swap_free_low_percentage_prop:s0
